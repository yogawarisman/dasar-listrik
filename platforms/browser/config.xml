<?xml version='1.0' encoding='utf-8'?>
<widget id="com.promozzing.rizal" version="1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Kolonialisme dan Imperialisme</name>
    <description>
        Media Pembelajaran Kolonialisme dan Imperialisme Bangsa Eropa di Indonesia
    </description>
    <author email="info@promozzing.com" href="http://promozzing.com">
        Promozzing
    </author>
    <content src="index.html" />
    <icon src="img/icon.png" />
    <splash src="res/screen/android/splash-port-hdpi.png" />
    <splash platform="android" qualifier="ldpi" src="res/screen/android/splash-port-ldpi.png" />
    <splash platform="android" qualifier="mdpi" src="res/screen/android/splash-port-mdpi.png" />
    <splash platform="android" qualifier="hdpi" src="res/screen/android/splash-port-hdpi.png" />
    <splash platform="android" qualifier="xhdpi" src="res/screen/android/splash-port-xhdpi.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
